{
	"name": "portfolio",
	"version": "0.0.1",
	"private": true,
	"browserslist": [
		"defaults, not op_mini all"
	],
	"scripts": {
		"start": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"lint:css": "stylelint '**/*.{css,scss,svelte}'",
		"format": "prettier --write .",
		"postinstall": "husky install"
	},
	"lint-staged": {
		"*.{ts,svelte}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{css,scss,svelte}": [
			"stylelint --fix"
		]
	},
	"dependencies": {
		"iso-3166-1": "^2.1.1",
		"just-throttle": "^4.2.0",
		"markdown-it": "^14.0.0",
		"moment": "^2.30.1",
		"puppeteer": "^21.7.0",
		"sketches": "portal:./../sketches",
		"workerpool": "^9.0.4"
	},
	"devDependencies": {
		"@kurone-kito/jsonresume-types": "^0.4.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/markdown-it": "^13.0.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-compat": "^4.2.0",
		"eslint-plugin-ecmascript-compat": "^3.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.0",
		"postcss": "^8.4.33",
		"postcss-html": "^1.6.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sass": "^1.69.7",
		"stylelint": "^16.1.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-recommended-scss": "^14.0.0",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-no-unsupported-browser-features": "^8.0.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^5.1.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"packageManager": "yarn@4.0.2"
}
